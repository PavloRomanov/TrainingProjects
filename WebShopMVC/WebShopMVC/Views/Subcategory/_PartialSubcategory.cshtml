@model IEnumerable<WebShop.Model.ViewModel.SubcategoryViewModel>

@{
    ViewBag.Title = "ListProductSubcategories";
}


<img src='/Pictures/Large home appliances.jpg' alt='product1' style='float:right ; width: 40%; height: 300px;'>
<br>
<img src='/Pictures/Small home appliances.jpg' alt='product2' style='float:right ; width: 40%; height: 300px;'>

  <ul class="subcategory">
    @foreach (var subcategory in Model)
    {
        <li>@Html.ActionLink(subcategory.SubcategoryName, "ProductsOfSubcategory", "Product",new { id = subcategory.SubcategoryId })</li>
    }
</ul>